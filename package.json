{
  "name": "metalist1925",
  "version": "1.0.0",
  "description": "metalist1925 on npm sripts with webpack and docker",
  "main": "server/app.js",
  "scripts": {
    "client:build": "NODE_ENV=development webpack",
    "server:build": "babel server --out-dir dist/server",
    "postserver:build": "npm run copy",
    "copy": "cp package.json dist/",
    "build": "npm run client:build && npm run server:build",
    "client:watch": "NODE_ENV=development webpack --watch",
    "server:watch": "babel server --watch --out-dir dist/server ",
    "watch": "npm run client:watch & npm run server:watch && rs",
    "clean": "rimraf dist/*",
    "start": "node dist/server",
    "serve": "node dist/server & npm run client:watch",
    "move": "mv server/pdfGenerator/OpenSans-Bold.ttf dist/server/pdfGenerator",
    "pnodemon": "npm run clean && npm run build",
    "prod": "npm run clean && npm run build && NODE_ENV=production node dist/server",
    "docker:prod": "NODE_ENV=production node server",
    "docker:dev": "NODE_ENV=development nodemon --watch server",
    "docker:build": "npm run clean && npm run build && sudo docker-compose -f docker-volumes.yml build",
    "lint": "jshint client/**/*.js server/**/*.js server/*.js",
    "test": "npm run server:build & NODE_ENV=test mocha dist/**/*.{spec,integration}.js --require mocha.conf.js",
    "predev1": "npm run clean -s && npm run build -s",
    "dev": "npm start",
    "nodemon": "nodemon --watch dist/server",
    "live-reload": "live-reload --port 9001 dist/server",
    "dev1": "npm run live-reload -s & npm start -s &npm run watch -s",
    "docker:build:dev": "npm run clean && npm run build && sudo docker-compose -f docker-compose-dev.yml -f docker-compose-dev.yml build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sash730/metalist1925.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sash730/metalist1925/issues"
  },
  "homepage": "https://github.com/Sash730/metalist1925#readme",
  "dependencies": {
    "angular": "^1.6.0",
    "angular-resource": "^1.6.0",
    "angular-cookies": "^1.6.0",
    "angular-sanitize": "^1.6.0",
    "angular-ui-bootstrap": "^1.3.3",
    "angular-ui-router": "^0.3.2",
    "angular-validation-match": "^1.5.2",
    "angular-datepicker": "^1.0.5",
    "bootstrap": "~3.1.1",
