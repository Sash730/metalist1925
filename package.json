{
  "name": "metalist1925",
  "version": "1.0.0",
  "description": "metalist1925 on npm sripts with webpack and docker",
  "main": "server/app.js",
  "scripts": {
    "test": "npm run build & NODE_ENV=test mocha dist/**/*.{spec,integration}.js --require mocha.conf.js",
    "serve": "NODE_ENV=development webpack --watch",
    "build:prod": "NODE_ENV=production webpack",
    "build:client": "NODE_ENV=development webpack",
    "build:server": "babel server --out-dir dist/server ",
    "clean": "rimraf dist/*",
    "move": "mv ../metalist/dist/client ./dist/client",
    "start": "NODE_ENV=development node server",
    "prod": "NODE_ENV=production node server",
    "docker:build": " npm run clean && npm run build && npm run move && sudo docker-compose -f docker-compose.yml build"

  },
